{
    "family": "simple-flask-task",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
      {
        "name": "flask-app",
        "image": "730335304128.dkr.ecr.us-east-1.amazonaws.com/simple-flask-app:latest",
        "portMappings": [
          {
            "containerPort": 5000,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ]
  }
  